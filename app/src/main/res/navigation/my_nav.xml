<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/adibFragment">

    <fragment
        android:id="@+id/adibFragment"
        android:name="com.example.adiblar_hayoti.bottom_fragments.AdibFragment"
        android:label="fragment_adib"
        tools:layout="@layout/fragment_adib" >
        <action
            android:id="@+id/action_adibFragment_to_savedFragment"
            app:destination="@id/savedFragment" />
        <action
            android:id="@+id/action_adibFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
    </fragment>
    <fragment
        android:id="@+id/savedFragment"
        android:name="com.example.adiblar_hayoti.bottom_fragments.SavedFragment"
        android:label="fragment_saved"
        tools:layout="@layout/fragment_saved" >
        <action
            android:id="@+id/action_savedFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.adiblar_hayoti.bottom_fragments.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" >
        <action
            android:id="@+id/action_settingsFragment_to_addFragment"
            app:destination="@id/addFragment" />
    </fragment>
    <fragment
        android:id="@+id/addFragment"
        android:name="com.example.adiblar_hayoti.fragments.AddFragment"
        android:label="fragment_add"
        tools:layout="@layout/fragment_add" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.adiblar_hayoti.fragments.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" />
    <fragment
        android:id="@+id/adib_ChildFragment"
        android:name="com.example.adiblar_hayoti.child.Adib_ChildFragment"
        android:label="fragment_adib__child"
        tools:layout="@layout/fragment_adib__child" />
</navigation>